{
  "expo": {
    "name": "GenCare",
    "slug": "gencare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "gencare",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.gencare"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ahmed_shams2.gencare",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "492afcaf-713b-4b0c-b955-f70442f2acdb"
      },
      "fonts": [
        {
          "asset": "assets/fonts/Fugaz_One/FugazOne-Regular.ttf",
          "family": "FugazOne"
        },
        {
          "asset": "assets/fonts/Actor/Actor-Regular.ttf",
          "family": "Actor"
        },
        {
          "asset": "assets/fonts/ADLaM_Display/ADLaMDisplay-Regular.ttf",
          "family": "ADLaMDisplay"
        }
      ]
    },
    "owner": "ahmed_shams2",
    "updates": {
      "url": "https://u.expo.dev/adbf0acd-f3da-4e6e-80ce-620c7f72c2f5"
    }
  }
}
